{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/about/?$",
			"dest": "_render"
		},
		{
			"src": "^/blog/?$",
			"dest": "_render"
		},
		{
			"src": "^/blog(?:/(.*?))?/?$",
			"dest": "_render"
		},
		{
			"src": "^/login/?$",
			"dest": "_render"
		},
		{
			"src": "^/rss\\.xml/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	],
	"images": {
		"sizes": [
			640,
			750,
			828,
			1080,
			1200,
			1920,
			2048,
			3840
		],
		"domains": [],
		"remotePatterns": []
	}
}